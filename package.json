{
    "name": "@budarin/postcss-pixelstorem",
    "version": "1.0.9",
    "description": "A plugin for PostCSS that generates rem units from pixel units.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "keywords": [
        "postcss",
        "plugin",
        "rem",
        "pixel",
        "storem"
    ],
    "author": "Vadim Budarin",
    "license": "MIT",
    "peerDependencies": {
        "postcss": "^8.0.0"
    },
    "devDependencies": {
        "postcss": "^8.0.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "build": "tsc",
        "type-check": "tsc --noEmit",
        "pnpm-install": "pnpm install",
        "upgrade-dependencies": "pnpm upgrade -i",
        "publish-package": "pnpm build && pnpm publish --access=public"
    },
    "packageManager": "pnpm@10.16.1"
}
